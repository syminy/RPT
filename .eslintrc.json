{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true,
    "jest": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "ignorePatterns": [
    "webui/static/app*.js",
    "webui/static/vue/**",
    "webui/static/*.min.js"
  ],
  "rules": {
    "no-console": "off",
    "no-unused-vars": ["warn", {"argsIgnorePattern": "^_"}]
  }
}
